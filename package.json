{
  "name": "geotracker",
  "version": "1.0.0",
  "main": "backend/src/index.ts",
  "scripts": {
    "watch": "tsc --watch",
    "backend": "node --watch build/index.js",
    "dev:backend": "concurrently \"npm run watch\" \"npm run backend\"",
    "dev:frontend": "npm start --prefix ./frontend",
    "check-types": "tsc",
    "prettier": "prettier --write .",
    "build:backend": "tsc",
    "build:frontend": "npm run build --prefix ./frontend",
    "build": "tsc && npm run build:frontend"
  },
  "repository": "https://github.com/Eduard-F/GeoTracker",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@sendgrid/mail": "^8.1.3",
    "@turf/turf": "^7.1.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.7",
    "argon2": "^0.40.1",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^8.4.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongo-sanitize": "^1.0.3",
    "@types/node": "^20.14.9",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "3.2.5",
    "typescript": "^5.5.2"
  }
}
